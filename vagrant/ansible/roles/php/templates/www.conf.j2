[www]

user = {{ php_fpm_user }}
group = {{ php_fpm_group }}

listen = /run/php/php7.3-fpm.sock
listen.owner = {{ php_fpm_user }}
listen.group = {{ php_fpm_group }}
listen.allowed_clients = 127.0.0.1
pm = dynamic
pm.max_children = 5
pm.start_servers = 2
pm.min_spare_servers = 1
pm.max_spare_servers = 3
pm.max_requests = 400